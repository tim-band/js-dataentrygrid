{
  "name": "js-dataentrygrid",
  "version": "2.2.0",
  "description": "A minimalist, no-dependency 2D grid for data input",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-file dist/dataentrygrid.min.js --minified --no-comments",
    "build-debug": "babel src --out-file dist/dataentrygrid.min.js",
    "docs": "documentation readme src/*.js --section=API",
    "test": "npm run build && mocha test/index.js",
    "test-firefox": "npm run build && mocha test/index.js -- --browser=firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-band/js-dataentrygrid.git"
  },
  "keywords": [
    "data",
    "grid",
    "entry",
    "data-grid",
    "table",
    "editor",
    "grid-editor",
    "data-table",
    "tabular-data",
    "edit-cell",
    "editable-table"
  ],
  "author": "Tim Band",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/tim-band/js-dataentrygrid/issues"
  },
  "homepage": "https://github.com/tim-band/js-dataentrygrid#readme",
  "dependencies": {
    "@babel/cli": "^7.14.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/generator": "^7.12.11",
    "@babel/preset-env": "^7.12.11",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "clipboardy": "^2.3.0",
    "core-js": "^3.8.2",
    "documentation": "^14.0.1",
    "mocha": "^9.2.0",
    "selenium-webdriver": "^4.0.0-alpha.8"
  }
}
